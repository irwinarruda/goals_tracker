<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AppBar">
        <attr name="appBar_title" format="string" />
        <attr name="appBar_coins" format="integer" />
    </declare-styleable>
    <declare-styleable name="IconButton">
        <attr name="iconButton_icon" format="reference" />
        <attr name="iconButton_iconSize" format="dimension" />
        <attr name="iconButton_contentDescription" format="string" />
    </declare-styleable>
    <declare-styleable name="DayButton">
        <attr name="dayButton_count" format="integer" />
        <attr name="dayButton_day" format="string" />
    </declare-styleable>
    <declare-styleable name="GoalSelectable">
        <attr name="goalSelectable_title" format="string" />
        <attr name="goalSelectable_duration" format="integer" />
        <attr name="goalSelectable_date" format="string" />
        <attr name="goalSelectable_coins" format="integer" />
    </declare-styleable>
    <declare-styleable name="GoalItem">
        <attr name="goalItem_title" format="string" />
        <attr name="goalItem_duration" format="integer" />
        <attr name="goalItem_date" format="string" />
        <attr name="goalItem_coins" format="integer" />
    </declare-styleable>
</resources>